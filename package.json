{
  "name": "agricultural-data-pipeline",
  "version": "1.0.0",
  "description": "Express.js-based agricultural sensor data processing pipeline with DuckDB",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docker:build": "docker build -t agricultural-data-pipeline .",
    "docker:run": "docker run -p 3000:3000 agricultural-data-pipeline",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f && docker volume prune -f"
  },
  "keywords": [
    "agriculture",
    "sensor-data",
    "pipeline",
    "express",
    "duckdb",
    "api"
  ],
  "author": "Umashankar",
  "license": "MIT",
  "dependencies": {
    "@dsnp/parquetjs": "^1.8.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "duckdb": "^1.3.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "fast-csv": "^4.3.6",
    "fs-extra": "^11.3.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "moment-timezone": "^0.6.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Iknowimhere/agri_monitoring_system.git"
  },
  "bugs": {
    "url": "https://github.com/Iknowimhere/agri_monitoring_system/issues"
  },
  "homepage": "https://github.com/Iknowimhere/agri_monitoring_system#readme"
}
